{
    "id": "http://dojotoolkit.org/dojox/mobile/RadioButton",
    "name": "dojox.mobile.RadioButton",
    "spec": "1.0",
    "version": "1.7",
    "require": [
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile",
            "$library": "dojo"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile/deviceTheme",
            "$library": "dojo"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile/RadioButton",
            "$library": "dojo"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../../dojo/dojo.js"
        }
    },
    "property": {
        "name": {
            "datatype": "string"
        },
        "baseClass": {
            "datatype": "string",
            "defaultValue": "mblRadioBox"
        },
        "duration": {
            "datatype": "number",
            "defaultValue": 1000
        },
        "checked": {
            "datatype": "boolean",
            "defaultValue": false
        },
        "value": {
            "datatype": "string",
            "defaultValue": "on"
        },
        "readOnly": {
            "datatype": "boolean",
            "defaultValue": false
        }
    },
    "content": "<input type='radio'>",
    "title": {
        "type": "text/html",
        "value": "<p>A non-templated radio button widget that can be in two states (checked or not checked).</p>"
    }
}