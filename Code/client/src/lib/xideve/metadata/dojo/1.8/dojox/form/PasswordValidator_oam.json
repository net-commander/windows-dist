{
    "id": "http://dojotoolkit.org/dojox/form/PasswordValidator",
    "name": "dojox.form.PasswordValidator",
    "spec": "1.0",
    "version": "1.0",
    "require": [
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/form/PasswordValidator",
            "$library": "dojo"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../../dojo/dojo.js"
        }
    },
    "property": {
        "type": {
            "datatype": "string",
            "defaultValue": "text",
            "hidden": true
        },
        "name": {
            "datatype": "string",
            "title": "Name"
        },
        "alt": {
            "datatype": "string",
            "hidden": true
        },
        "value": {
            "datatype": "string",
            "hidden": true
        },
        "tabIndex": {
            "datatype": "string",
            "defaultValue": "0",
            "title": "Tab Index"
        },
        "disabled": {
            "datatype": "boolean",
            "title": "Disabled"
        },
        "readOnly": {
            "datatype": "boolean",
            "hidden": true
        },
        "intermediateChanges": {
            "datatype": "boolean",
            "hidden": true
        },
        "required": {
            "datatype": "boolean",
            "defaultValue": true,
            "title": "Required"
        },
        "oldName": {
            "datatype": "string",
            "hidden": true
        },
        "scrollOnFocus": {
            "datatype": "boolean",
            "description": "On focus, should this widget scroll into view?",
            "hidden": false
        }
    },
    "content": "<span></span>",
    "title": {
        "type": "text/html",
        "value": "<p>A password validation widget that simplifies the &quot;old/new/verify&quot;\nstyle of requesting passwords.  You will probably want to override\nthis class and implement your own pwCheck function.</p>"
    }
}