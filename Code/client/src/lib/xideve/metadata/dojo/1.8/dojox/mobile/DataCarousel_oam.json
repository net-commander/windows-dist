{
    "id": "http://dojotoolkit.org/dojox/mobile/DataCarousel",
    "name": "dojox.mobile.DataCarousel",
    "spec": "1.0",
    "version": "1.7",
    "require": [
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile",
            "$library": "dojo"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile/deviceTheme",
            "$library": "dojo"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile/DataCarousel",
            "$library": "dojo"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../../dojo/dojo.js"
        }
    },
    "property": {
        "numVisible": {
            "datatype": "number",
            "defaultValue": 3
        },
        "title": {
            "datatype": "string"
        },
        "pageIndicator": {
            "datatype": "boolean",
            "defaultValue": true
        },
        "navButton": {
            "datatype": "boolean",
            "defaultValue": false
        },
        "height": {
            "datatype": "string",
            "defaultValue": "300px"
        },
        "store": {
            "datatype": "object",
            "hidden": true
        },
        "query": {
            "datatype": "json",
            "hidden": true
        },
        "queryOptions": {
            "datatype": "json",
            "hidden": true
        }
    },
    "content": "<div></div>",
    "title": {
        "type": "text/html",
        "value": "<p>A dojo/data-enabled Carousel.</p>"
    },
    "description": {
        "type": "text/html",
        "value": "<p>DataCarousel is an enhanced version of dojox/mobile/Carousel. It\ncan generate contents according to the given dojo/data store.</p>"
    }
}