{
    "id": "http://dojotoolkit.org/dojox/mobile/IconMenuItem",
    "name": "dojox.mobile.IconMenuItem",
    "spec": "1.0",
    "version": "1.8",
    "require": [
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile",
            "$library": "dojo"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile/deviceTheme",
            "$library": "dojo"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile/IconMenuItem",
            "$library": "dojo"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../../dojo/dojo.js"
        }
    },
    "property": {
        "transition": {
            "datatype": "string",
            "defaultValue": "slide",
            "option": [
                {
                    "value": "slide"
                },
                {
                    "value": "fade"
                },
                {
                    "value": "flip"
                },
                {
                    "value": "cover"
                },
                {
                    "value": "coverv"
                },
                {
                    "value": "dissolve"
                },
                {
                    "value": "reveal"
                },
                {
                    "value": "revealv"
                },
                {
                    "value": "scaleIn"
                },
                {
                    "value": "scaleOut"
                },
                {
                    "value": "slidev"
                },
                {
                    "value": "swirl"
                },
                {
                    "value": "zoomIn"
                },
                {
                    "value": "zoomOut"
                }
            ]
        },
        "transitionDir": {
            "datatype": "number",
            "defaultValue": 1
        },
        "icon": {
            "datatype": "string"
        },
        "iconPos": {
            "datatype": "string"
        },
        "moveTo": {
            "datatype": "string"
        },
        "href": {
            "datatype": "string"
        },
        "hrefTarget": {
            "datatype": "string"
        },
        "selected": {
            "datatype": "boolean"
        },
        "url": {
            "datatype": "string"
        },
        "urlTarget": {
            "datatype": "string"
        },
        "callback": {
            "datatype": "string"
        },
        "label": {
            "datatype": "string",
            "defaultValue": "IconMenuItem"
        },
        "alt": {
            "datatype": "string"
        },
        "tabindex": {
            "datatype": "string",
            "defaultValue": "0"
        }
    },
    "content": "<li label='IconMenuItem'></li>",
    "title": {
        "type": "text/html",
        "value": "<p>An item of IconMenu.</p>"
    },
    "description": {
        "type": "text/html",
        "value": "<p>IconMenuItem represents a menu item of\ndojox/mobile/MenuItem. This widget inherits from\ndojox/mobile/_ItemBase. Basic usage is same as the other\nsubclasses such as dojox/mobile/ListItem.</p>"
    }
}