{
    "id": "http://dojotoolkit.org/dojox/mobile/TextBox",
    "name": "dojox.mobile.TextBox",
    "spec": "1.0",
    "version": "1.7",
    "require": [
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile",
            "$library": "dojo"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile/deviceTheme",
            "$library": "dojo"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile/TextBox",
            "$library": "dojo"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../../dojo/dojo.js"
        }
    },
    "property": {
        "name": {
            "datatype": "string"
        },
        "alt": {
            "datatype": "string"
        },
        "value": {
            "datatype": "string",
            "description": "text that the widget displays inside the input, note the placeHolder will not be displayed if a value is specified."

        },
        "tabIndex": {
            "datatype": "number",
            "defaultValue": 0
        },
        "disabled": {
            "datatype": "boolean",
            "defaultValue": false
        },
        "intermediateChanges": {
            "datatype": "boolean",
            "defaultValue": false
        },
        "scrollOnFocus": {
            "datatype": "boolean",
            "defaultValue": true
        },
        "readOnly": {
            "datatype": "boolean",
            "defaultValue": false
        },
        "trim": {
            "datatype": "boolean",
            "defaultValue": false
        },
        "uppercase": {
            "datatype": "boolean",
            "defaultValue": false
        },
        "lowercase": {
            "datatype": "boolean",
            "defaultValue": false
        },
        "propercase": {
            "datatype": "boolean",
            "defaultValue": false
        },
        "maxLength": {
            "datatype": "string"
        },
        "selectOnClick": {
            "datatype": "boolean",
            "defaultValue": false
        },
        "placeHolder": {
            "datatype": "string",
            "description": "hint text that the widget displays inside the input area of empty form fields, such as 'Your Name'. "
        },
        "baseClass": {
            "datatype": "string",
            "defaultValue": "mblTextBox"
        }
    },
    "content": "<input type='text'>",
    "title": {
        "type": "text/html",
        "value": "<p>A non-templated base class for textbox form inputs</p>"
    }
}