{
    "id": "http://dojotoolkit.org/dojox/mobile/CheckBox",
    "name": "dojox.mobile.CheckBox",
    "spec": "1.0",
    "version": "1.7",
    "require": [
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile",
            "$library": "dojo"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile/deviceTheme",
            "$library": "dojo"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile/CheckBox",
            "$library": "dojo"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../../dojo/dojo.js"
        }
    },
    "property": {
        "baseClass": {
            "datatype": "string",
            "defaultValue": "mblCheckBox"
        },
        "duration": {
            "datatype": "number",
            "defaultValue": 1000
        },
        "checked": {
            "datatype": "boolean",
            "defaultValue": false
        },
        "value": {
            "datatype": "string",
            "defaultValue": "on"
        },
        "readOnly": {
            "datatype": "boolean",
            "defaultValue": false
        }
    },
    "content": "<input type='checkbox'>",
    "title": {
        "type": "text/html",
        "value": "<p>A non-templated checkbox widget that can be in two states \n(checked or not checked).</p>"
    }
}