{
    "id": "http://dojotoolkit.org/dojox/mobile/ProgressIndicator",
    "name": "dojox.mobile.ProgressIndicator",
    "spec": "1.0",
    "version": "1.8",
    "require": [
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile",
            "$library": "dojo"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile/deviceTheme",
            "$library": "dojo"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile/ProgressIndicator",
            "$library": "dojo"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../../dojo/dojo.js"
        }
    },
    "property": {
        "interval": {
            "datatype": "number",
            "defaultValue": 100
        },
        "size": {
            "datatype": "number",
            "defaultValue": 40
        },
        "removeOnStop": {
            "datatype": "boolean",
            "defaultValue": true
        },
        "startSpinning": {
            "datatype": "boolean",
            "defaultValue": false
        },
        "center": {
            "datatype": "boolean",
            "defaultValue": true
        }
    },
    "content": "<div></div>",
    "title": {
        "type": "text/html",
        "value": "<p>A progress indication widget.</p>"
    },
    "description": {
        "type": "text/html",
        "value": "<p>ProgressIndicator is a round spinning graphical representation\nthat indicates the current task is ongoing.</p>"
    }
}