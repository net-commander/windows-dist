{
    "id": "http://dojotoolkit.org/dojox/mobile/TabButton",
    "name": "dojox.mobile.TabButton",
    "spec": "1.0",
    "version": "1.7",
    "require": [
        {
            "type": "css",
            "src": "dojox/mobile/themes/common/domButtons/DomButtonRedBadge.css",
            "$library": "dojo"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile",
            "$library": "dojo"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile/deviceTheme",
            "$library": "dojo"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile/TabBarButton",
            "$library": "dojo"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../../dojo/dojo.js"
        }
    },
    "property": {
        "transition": {
            "datatype": "string",
            "defaultValue": "none",
            "option": [
                {
                    "value": "slide"
                },
                {
                    "value": "fade"
                },
                {
                    "value": "flip"
                },
                {
                    "value": "cover"
                },
                {
                    "value": "coverv"
                },
                {
                    "value": "dissolve"
                },
                {
                    "value": "reveal"
                },
                {
                    "value": "revealv"
                },
                {
                    "value": "scaleIn"
                },
                {
                    "value": "scaleOut"
                },
                {
                    "value": "slidev"
                },
                {
                    "value": "swirl"
                },
                {
                    "value": "zoomIn"
                },
                {
                    "value": "zoomOut"
                }
            ]
        },
        "transitionDir": {
            "datatype": "number",
            "defaultValue": 1
        },
        "icon1": {
            "datatype": "string"
        },
        "icon2": {
            "datatype": "string"
        },
        "iconPos1": {
            "datatype": "string"
        },
        "iconPos2": {
            "datatype": "string"
        },
        "moveTo": {
            "datatype": "string"
        },
        "href": {
            "datatype": "string"
        },
        "hrefTarget": {
            "datatype": "string"
        },
        "callback": {
            "datatype": "string"
        },
        "label": {
            "datatype": "string",
            "defaultValue": "TabButton"
        },
        "badge": {
            "datatype": "string"
        },
        "selected": {
            "datatype": "boolean",
            "defaultValue": false
        }
    },
    "content": "<li label='TabButton'></li>",
    "title": {
        "type": "text/html",
        "value": "<p>A button widget that is placed in the TabBar widget.</p>"
    },
    "description": {
        "type": "text/html",
        "value": "<p>TabBarButton is a button that is placed in the TabBar widget. It\nis a subclass of dojox/mobile/_ItemBase just like ListItem or\nIconItem. So, unlike Button, it has similar capability as\nListItem or IconItem, such as icon support, transition, etc.</p>"
    }
}