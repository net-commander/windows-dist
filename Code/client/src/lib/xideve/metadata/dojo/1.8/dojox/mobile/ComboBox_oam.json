{
    "id": "http://dojotoolkit.org/dojox/mobile/ComboBox",
    "name": "dojox.mobile.ComboBox",
    "spec": "1.0",
    "version": "1.7",
    "require": [
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile",
            "$library": "dojo"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile/deviceTheme",
            "$library": "dojo"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile/ComboBox",
            "$library": "dojo"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../../dojo/dojo.js"
        }
    },
    "property": {
        "selectOnClick": {
            "datatype": "boolean",
            "defaultValue": false
        },
        "autoComplete": {
            "datatype": "boolean",
            "defaultValue": false
        }
    },
    "content": "<input type='text'>",
    "title": {
        "type": "text/html",
        "value": "<p>A non-templated auto-completing text box widget.</p>"
    }
}