{
    "id": "http://dojotoolkit.org/dojox/charting/plot2d/Bubble",
    "name": "dojox.charting.plot2d/Bubble",
    "spec": "1.0",
    "version": "1.0",
    "require": [
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/charting/plot2d/Bubble",
            "$library": "dojo"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../../../dojo/dojo.js"
        }
    },
    "property": {
        "hAxis": {
            "datatype": "string"
        },
        "vAxis": {
            "datatype": "string"
        },
        "shadows": {
            "datatype": "array",
            "title": "Shadows"
        }
    },
    "content": "<span></span>",
    "title": {
        "type": "text/html",
        "value": "<p>A plot representing bubbles.  Note that data for Bubbles requires 3 parameters,\nin the form of:  { x, y, size }, where size determines the size of the bubble.</p>"
    }
