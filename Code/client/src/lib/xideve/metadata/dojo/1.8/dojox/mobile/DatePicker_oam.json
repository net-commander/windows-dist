{
    "id": "http://dojotoolkit.org/dojox/mobile/DatePicker",
    "name": "dojox.mobile.DatePicker",
    "spec": "1.0",
    "version": "1.8",
    "require": [
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile",
            "$library": "dojo"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile/deviceTheme",
            "$library": "dojo"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile/DatePicker",
            "$library": "dojo"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../../dojo/dojo.js"
        }
    },
    "property": {
        "yearPattern": {
            "datatype": "string",
            "defaultValue": "YYYY"
        },
        "monthPattern": {
            "datatype": "string",
            "defaultValue": "MMM"
        },
        "dayPattern": {
            "datatype": "string",
            "defaultValue": "d"
        }
    },
    "content": "<div></div>",
    "title": {
        "type": "text/html",
        "value": "<p>A wrapper widget around SpinWheelDatePicker or ValuePickerDatePicker.\nReturns ValuePickerDatePicker when the current theme is &quot;android&quot;.\nReturns SpinWheelDatePicker otherwise.</p>"
    }
}