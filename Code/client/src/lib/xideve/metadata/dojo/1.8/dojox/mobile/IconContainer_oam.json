{
    "id": "http://dojotoolkit.org/dojox/mobile/IconContainer",
    "name": "dojox.mobile.IconContainer",
    "spec": "1.0",
    "version": "1.7",
    "require": [
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile",
            "$library": "dojo"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile/deviceTheme",
            "$library": "dojo"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile/IconContainer",
            "$library": "dojo"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../../dojo/dojo.js"
        }
    },
    "property": {
        "defaultIcon": {
            "datatype": "string"
        },
        "pressedIconOpacity": {
            "datatype": "number",
            "defaultValue": 0.4
        },
        "transition": {
            "datatype": "string",
            "defaultValue": "below"
        },
        "iconBase": {
            "datatype": "string"
        },
        "iconPos": {
            "datatype": "string"
        },
        "back": {
            "datatype": "string",
            "defaultValue": "Home"
        },
        "label": {
            "datatype": "string",
            "defaultValue": "My Application"
        },
        "single": {
            "datatype": "boolean",
            "defaultValue": false
        },
        "editable": {
            "datatype": "boolean",
            "defaultValue": false
        }
    },
    "content": "<ul></ul>",
    "title": {
        "type": "text/html",
        "value": "<p>A container widget which can hold multiple icons.</p>"
    },
    "description": {
        "type": "text/html",
        "value": "<p>IconContainer is a container widget which can hold multiple\nicons. Each icon represents an application component.</p>"
    }
}