{
    "id": "http://dojotoolkit.org/dojox/mobile/IconItem",
    "name": "dojox.mobile.IconItem",
    "spec": "1.0",
    "version": "1.7",
    "require": [
        {
            "type": "css",
            "src": "dojox/mobile/themes/common/domButtons/DomButtonRedBadge.css",
            "$library": "dojo"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile",
            "$library": "dojo"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile/deviceTheme",
            "$library": "dojo"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile/IconItem",
            "$library": "dojo"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../../dojo/dojo.js"
        }
    },
    "property": {
        "transition": {
            "datatype": "string",
            "defaultValue": "slide"
        },
        "transitionDir": {
            "datatype": "number",
            "defaultValue": 1
        },
        "icon": {
            "datatype": "string"
        },
        "iconPos": {
            "datatype": "string"
        },
        "moveTo": {
            "datatype": "string"
        },
        "href": {
            "datatype": "string"
        },
        "hrefTarget": {
            "datatype": "string"
        },
        "callback": {
            "datatype": "string"
        },
        "label": {
            "datatype": "string",
            "defaultValue": "IconItem"
        },
        "lazy": {
            "datatype": "boolean",
            "defaultValue": false
        },
        "requires": {
            "datatype": "string"
        },
        "timeout": {
            "datatype": "number",
            "defaultValue": 10
        },
        "badge": {
            "datatype": "string"
        }
    },
    "content": "<li label='IconItem'></li>",
    "title": {
        "type": "text/html",
        "value": "<p>An icon item widget.</p>"
    },
    "description": {
        "type": "text/html",
        "value": "<p>IconItem represents an item that has an application component\nand its icon image. You can tap the icon to open the\ncorresponding application component. You can also use the icon\nto move to a different view by specifying either of the moveTo,\nhref or url parameters.</p>"
    }
}