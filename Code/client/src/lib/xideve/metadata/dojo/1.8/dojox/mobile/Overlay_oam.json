{
    "id": "http://dojotoolkit.org/dojox/mobile/Overlay",
    "name": "dojox.mobile.Overlay",
    "spec": "1.0",
    "version": "1.7",
    "require": [
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile",
            "$library": "dojo"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile/deviceTheme",
            "$library": "dojo"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile/Overlay",
            "$library": "dojo"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../../dojo/dojo.js"
        }
    },
    "property": {
        "baseClass": {
            "datatype": "string",
            "defaultValue": "mblOverlay mblOverlayHidden"
        }
    },
    "content": "<div></div>",
    "title": {
        "type": "text/html",
        "value": "<p>A non-templated widget that animates up from the bottom, \noverlaying the current content.</p>"
    }
}