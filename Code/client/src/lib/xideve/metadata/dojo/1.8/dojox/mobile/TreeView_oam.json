{
    "id": "http://dojotoolkit.org/dojox/mobile/TreeView",
    "name": "dojox.mobile.TreeView",
    "spec": "1.0",
    "version": "1.8",
    "require": [
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile",
            "$library": "dojo"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile/deviceTheme",
            "$library": "dojo"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile/TreeView",
            "$library": "dojo"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../../dojo/dojo.js"
        }
    },
    "property": {
        "store": {
            "datatype": "object",
            "hidden": true,
            "isData": true
        },
        "rootLabel": {
            "datatype": "string"
        },
        "childrenAttrs": {
            "datatype": "array"
        },
        "newItemIdAttr": {
            "datatype": "string"
        }
    },
    "content": "<div></div>",
    "title": {
        "type": "text/html",
        "value": "<p>A scrollable view with tree-style navigation.</p>"
    },
    "description": {
        "type": "text/html",
        "value": "<p>This widget can be connected to a dojox/data/FileStore as a\nquick directory browser. You may use it when implementing the\nMaster-Detail pattern.</p>"
    }
}