{
    "id": "http://dojotoolkit.org/dojox/mobile/ScrollablePane",
    "name": "dojox.mobile.ScrollablePane",
    "spec": "1.0",
    "version": "1.8",
    "require": [
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile",
            "$library": "dojo"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile/deviceTheme",
            "$library": "dojo"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile/ScrollablePane",
            "$library": "dojo"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../../dojo/dojo.js"
        }
    },
    "property": {
        "roundCornerMask": {
            "datatype": "boolean",
            "defaultValue": false
        },
        "radius": {
            "datatype": "number",
            "defaultValue": 0
        }
    },
    "content": "<div></div>",
    "title": {
        "type": "text/html",
        "value": "<p>A pane that has the touch-scrolling capability.</p>"
    }
}