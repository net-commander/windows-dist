{
    "id": "http://dojotoolkit.org/dojox/form/Rating",
    "name": "dojox.form.Rating",
    "spec": "1.0",
    "version": "1.0",
    "require": [
        {
            "type": "css",
            "src": "dojox/form/resources/Rating.css",
            "$library": "dojo"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/form/Rating",
            "$library": "dojo"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../../dojo/dojo.js"
        }
    },
    "property": {
        "type": {
            "datatype": "string",
            "defaultValue": "text",
            "hidden": true
        },
        "name": {
            "datatype": "string",
            "title": "Name"
        },
        "alt": {
            "datatype": "string",
            "hidden": true
        },
        "value": {
            "datatype": "number",
            "defaultValue": 0,
            "title": "Value"
        },
        "tabIndex": {
            "datatype": "string",
            "defaultValue": "0",
            "title": "Tab Index"
        },
        "disabled": {
            "datatype": "boolean",
            "title": "Disabled"
        },
        "readOnly": {
            "datatype": "boolean",
            "title": "Read Only"
        },
        "intermediateChanges": {
            "datatype": "boolean",
            "hidden": true
        },
        "numStars": {
            "datatype": "number",
            "title": "Number of stars"
        },
        "scrollOnFocus": {
            "datatype": "boolean",
            "description": "On focus, should this widget scroll into view?",
            "hidden": false
        }
    },
    "content": "<span></span>",
    "title": {
        "type": "text/html",
        "value": "<p>A widget for rating using stars.</p>"
    }
}