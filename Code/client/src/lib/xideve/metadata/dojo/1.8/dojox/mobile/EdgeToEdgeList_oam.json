{
    "id": "http://dojotoolkit.org/dojox/mobile/EdgeToEdgeList",
    "name": "dojox.mobile.EdgeToEdgeList",
    "spec": "1.0",
    "version": "1.7",
    "require": [
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile",
            "$library": "dojo"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile/deviceTheme",
            "$library": "dojo"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../../dojo/dojo.js"
        }
    },
    "property": {
        "transition": {
            "datatype": "string",
            "defaultValue": "slide",
            "option": [
                {
                    "value": "slide"
                },
                {
                    "value": "fade"
                },
                {
                    "value": "flip"
                },
                {
                    "value": "cover"
                },
                {
                    "value": "coverv"
                },
                {
                    "value": "dissolve"
                },
                {
                    "value": "reveal"
                },
                {
                    "value": "revealv"
                },
                {
                    "value": "scaleIn"
                },
                {
                    "value": "scaleOut"
                },
                {
                    "value": "slidev"
                },
                {
                    "value": "swirl"
                },
                {
                    "value": "zoomIn"
                },
                {
                    "value": "zoomOut"
                }
            ]
        },
        "iconBase": {
            "datatype": "string"
        },
        "iconPos": {
            "datatype": "string"
        },
        "select": {
            "datatype": "string",
            "defaultValue": "",
            "option": [
                {
                    "value": ""
                },
                {
                    "value": "single"
                },
                {
                    "value": "multiple"
                }
            ]
        },
        "editable": {
            "datatype": "boolean",
            "defaultValue": false
        }
    },
    "content": "<ul></ul>",
    "title": {
        "type": "text/html",
        "value": "<p>An edge-to-edge layout list.</p>"
    },
    "description": {
        "type": "text/html",
        "value": "<p>EdgeToEdgeList is an edge-to-edge layout list, which displays\nall items in equally-sized rows. Each item must be a\ndojox/mobile/ListItem.</p>"
    }
}