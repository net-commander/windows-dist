{
    "id": "http://dojotoolkit.org/dojox/mobile/SearchBox",
    "name": "dojox.mobile.SearchBox",
    "spec": "1.0",
    "version": "1.8",
    "require": [
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile",
            "$library": "dojo"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile/deviceTheme",
            "$library": "dojo"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile/SearchBox",
            "$library": "dojo"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../../dojo/dojo.js"
        }
    },
    "property": {
        "trim": {
            "datatype": "boolean",
            "defaultValue": false
        },
        "maxLength": {
            "datatype": "string"
        },
        "placeHolder": {
            "datatype": "string"
        },
        "baseClass": {
            "datatype": "string",
            "defaultValue": "mblSearchBox"
        },
        "pageSize": {
            "datatype": "number"
        },
        "store": {
            "datatype": "object",
            "hidden": true,
            "isData": true
        },
        "fetchProperties": {
            "datatype": "json",
            "hidden": true
        },
        "query": {
            "datatype": "json",
            "hidden": true
        },
        "searchDelay": {
            "datatype": "number",
            "defaultValue": 200
        },
        "searchAttr": {
            "datatype": "string",
            "defaultValue": "name"
        },
        "queryExpr": {
            "datatype": "string",
            "defaultValue": "${0}*"
        },
        "ignoreCase": {
            "datatype": "boolean",
            "defaultValue": false
        },
        "incremental": {
            "datatype": "boolean",
            "defaultValue": true
        }
    },
    "content": "<input type='text'>",
    "title": {
        "type": "text/html",
        "value": "<p>A non-templated base class for INPUT type=&quot;search&quot;.</p>"
    }
}