{
    "id": "http://dojotoolkit.org/dojox/mobile/ToggleButton",
    "name": "dojox.mobile.ToggleButton",
    "spec": "1.0",
    "version": "1.7",
    "require": [
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile",
            "$library": "dojo"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile/deviceTheme",
            "$library": "dojo"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile/ToggleButton",
            "$library": "dojo"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../../dojo/dojo.js"
        }
    },
    "property": {
        "btnClass": {
            "datatype": "string",
            "defaultValue": "mblToggleButton"
        },
        "duration": {
            "datatype": "number",
            "defaultValue": 1000
        },
        "label": {
            "datatype": "string",
            "defaultValue": "ToggleButton"
        },
        "checked": {
            "datatype": "boolean",
            "defaultValue": false
        },
        "disabled": {
            "datatype": "boolean",
            "defaultValue": false
        }
    },
    "content": "<button label='ToggleButton'></button>",
    "title": {
        "type": "text/html",
        "value": "<p>A non-templated button widget that can be in two states (checked or not).\nCan be base class for things like tabs or checkbox or radio buttons</p>"
    }
}