{
    "id": "http://dojotoolkit.org/dojox/mobile/ValuePickerDatePicker",
    "name": "dojox.mobile.ValuePickerDatePicker",
    "spec": "1.0",
    "version": "1.8",
    "require": [
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile",
            "$library": "dojo"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile/deviceTheme",
            "$library": "dojo"
        },
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "dojox/mobile/ValuePickerDatePicker",
            "$library": "dojo"
        }
    ],
    "library": {
        "dojo": {
            "src": "../../../dojo/dojo.js"
        }
    },
    "property": {
        "yearPattern": {
            "datatype": "string",
            "defaultValue": "YYYY"
        },
        "monthPattern": {
            "datatype": "string",
            "defaultValue": "MMM"
        },
        "dayPattern": {
            "datatype": "string",
            "defaultValue": "d"
        },
        "readOnly": {
            "datatype": "boolean",
            "defaultValue": false
        },
        "value": {
            "datatype": "string"
        }
    },
    "content": "<div></div>",
    "title": {
        "type": "text/html",
        "value": "<p>A ValuePicker-based date picker widget.</p>"
    },
    "description": {
        "type": "text/html",
        "value": "<p>ValuePickerDatePicker is a date picker widget. It is a subclass of\ndojox/mobile/ValuePicker. It has 3 slots: day, month and year.</p>"
    }
}