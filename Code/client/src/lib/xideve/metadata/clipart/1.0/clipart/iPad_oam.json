{
    "id": "http://www.maqetta.org/clipart/iPad",
    "name": "iPad",
    "spec": "1.0",
    "version": "1.0",
    "library": {
        "clipart": {
            "src": "../../../clipart"
        },
        "dojo": {
            "src": "../../../dojo/dojo.js"
        }
    },
    "require": [
        {
            "type": "javascript-module",
            "format": "amd",
            "src": "clipart/iPad",
            "$library": "clipart"
        },
        {
            "type": "image",
            "src": "iPad",
            "$library": "clipart"
        }
    ],
    "property": {
        "orientation": {
            "datatype": "string",
            "option": [
                {
                    "value": "portrait"
                },
                {
                    "value": "landscape"
                }
            ],
            "defaultValue": "portrait",
            "title": "Orientation"
        },
        "preserveAspectRatio": {
            "datatype": "boolean",
            "defaultValue": "true",
            "title": "Preserve aspect ratio"
        }
    },
    "content": "<div></div>",
    "title": {
        "type": "text/html",
        "value": "<p>Clipart icon: iPad</p>"
    }
}