{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "allowJs": false,
    "noImplicitAny": false,
    "sourceMap": true,
    "baseUrl": "./src",
    "outDir": "./",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "pretty": true,
    "declaration": true
  },
  "compileOnSave": false,
  
  "filesGlob": [
    "./src/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ],
  "files": [
    "./src/primitives.ts",
    "./src/collections.ts",
    "./src/map.ts",
    "./src/iterator.ts",
    "./src/set.ts",
    "./src/numbers.ts",
    "./src/collections.ts",
    "./src/strings.ts",
    "./src/assert.ts",
    "./src/arrays.ts",
    "./src/set.ts",
    "./src/objects.ts",
    "./src/utils.ts",
    "./src/index.ts",
    "./src/has.ts",
    "./src/promisify.ts",
    "./src/shim/Symbol.ts",
    "./src/shim/object.ts"
  ]
}
