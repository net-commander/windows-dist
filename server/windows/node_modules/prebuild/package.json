    "mkdirp": "^0.5.1",
    "node-gyp": "^3.0.3",
    "node-ninja": "^1.0.1",
    "noop-logger": "^0.1.0",
    "npmlog": "^2.0.0",
    "os-homedir": "^1.0.1",
    "pump": "^1.0.0",
