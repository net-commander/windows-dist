    "test": "tape test && npm run audit && npm run lint",
    "audit": "nsp check",
    "lint": "standard"
  },
  "keywords": [
    "prebuilt",
    "binaries",
    "native",
    "addon",
    "module",
    "c",
    "c++",
    "bindings",
    "devops"
  ],
