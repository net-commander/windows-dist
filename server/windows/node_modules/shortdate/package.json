{
  "name": "shortdate",
  "version": "1.2.0",
  "description": "get short date",
  "main": "lib/shortdate.js",
  "scripts": {
    "bundle": "browserify lib/shortdate.js -s shortdate --ng false > dist/shortdate.js",
    "build": "npm run bundle && npm run minify",
    "minify": "minify dist/shortdate.js > dist/shortdate.min.js",
    "wisdom": "npm run build",
    "codestyle": "jshint lib test && jscs --esnext lib test",
    "coverage": "nyc npm test",
    "report": "nyc report --reporter=text-lcov | coveralls",
    "test": "tape test/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/coderaiser/shortdate"
  },
  "keywords": [
    "date"
  ],
  "author": "coderaiser <mnemonic.enemy@gmail.com> (http://coderaiser.github.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/coderaiser/shortdate/issues"
  },
  "homepage": "https://github.com/coderaiser/shortdate",
  "devDependencies": {
    "browserify": "^13.0.0",
    "coveralls": "^2.11.6",
    "jscs": "^2.10.1",
    "jshint": "^2.9.1",
    "minify": "^2.0.3",
    "nyc": "^5.6.0",
    "tape": "^4.4.0"
  }
}
